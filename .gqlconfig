/** Requires:
 * playlfe/gql (nodejs)
 * Facebook Watchman (windows binary)
**/
{
  schema: {
    /** files: 'client/src/lib/types/schema.graphql' **/
    
        files: 'client/src/**/*.gql'
  },
  query: {
    files: [
      {
        match: 'client/src/**/*.gql',
        parser: 'QueryParser',
      },
    ]
  }
}