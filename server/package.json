{
  "name": "inventory-manager-daemon",
  "version": "0.0.1",
  "description": "Server for web resources and print handler via IPP",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "run": "node main.js",
    "watch-server": "nodemon --watch 'server/**/*' -e ts,tsx --exec ts-node ./server/index.ts",
    "install-run-server": "npm install && npm run watch-server",
    "client-start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "install-run-client": "npm install && npm run client-start"
  },
