{
    "name": "client",
    "version": "0.1.0-20210221001",
    "description": "Manage inventory, orders, labels",
    "main": "bundle.js",
    "scripts": {
        "build": "yarn build-clean",
        "build-dirty": "dotenv -e ../.env -- webpack --env NODE_ENV=production --config webpack.config.prod.js --progress --color",
        "build-clean": "remove-item -force -recurse ./dist/* && yarn build-dirty",
        "run-dev-server": "dotenv -e ../.env -- webpack serve",
        "serve": "node ./bundle.js",
        "codegen": "dotenv -e ../.env -- graphql-codegen --config codegen.yml",
        "test": "jest",
        "check-outdated": "yarn outdated",
        "example-install": "yarn add --latest --exact apollo-link-http",
        "example-upgrade": "yarn upgrade --latest --exact apollo-link-http",
        "upgrade-codegen": "yarn upgrade --latest --exact @graphql-codegen/cli @graphql-codegen/introspection @graphql-codegen/time @graphql-codegen/typescript @graphql-codegen/typescript-document-nodes @graphql-codegen/typescript-operations @graphql-codegen/typescript-react-apollo",
        "upgrade-deps": "yarn upgrade --latest --exact uuid",
        "upgrade-devDeps": "yarn upgrade --latest --exact css-loader html-webpack-plugin node-sass react-hot-loader sass-loader source-map-loader style-loader ts-loader typescript",
        "upgrade-webpack": "yarn upgrade --latest --exact webpack webpack-cli webpack-dev-server",
        "generate-json-schema": ".\\node_modules\\.bin\\ts-json-schema-generator --path .\\src\\lib\\item\\ItemHardwareFastenerScrewMachine\\*d.ts --tsconfig .\\tsconfig.json --type ScrewSizeSchema -o .\\src\\lib\\item\\ItemHardwareFastenerScrewMachine\\config\\ScrewSizeOptions.schema.json"
    },
