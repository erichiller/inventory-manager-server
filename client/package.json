  "description": "Server for web resources and print handler via IPP",
  "main": "bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "run-dev-server": "webpack-dev-server",
    "serve": "node ./bundle.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "check-outdated": "yarn outdated",
    "example-install": "yarn add --latest --exact apollo-link-http",
    "example-upgrade": "yarn upgrade --latest --exact apollo-link-http",
    "clean-imports": "tslint --config tslint-imports.json --fix --project .",
