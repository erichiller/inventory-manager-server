{
  "name": "inventory-manager-client",
  "version": "0.0.1",
  "description": "Server for web resources and print handler via IPP",
  "main": "bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "run-dev-server": "webpack-dev-server",
    "serve": "node ./bundle.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "check-outdated": "yarn outdated",
    "example-install": "yarn add --latest --exact apollo-link-http",
    "example-upgrade": "yarn upgrade --latest --exact apollo-link-http",
    "clean-imports": "tslint --config tslint-imports.json --fix --project .",
    "upgrade-codegen": "yarn upgrade --latest --exact @graphql-codegen/cli @graphql-codegen/introspection @graphql-codegen/time @graphql-codegen/typescript @graphql-codegen/typescript-document-nodes @graphql-codegen/typescript-operations @graphql-codegen/typescript-react-apollo",
    "upgrade-deps": "yarn upgrade --latest --exact uuid",
    "upgrade-devDeps": "yarn upgrade --latest --exact css-loader html-webpack-plugin node-sass react-hot-loader sass-loader source-map-loader style-loader ts-loader tslint typescript",
    "upgrade-webpack": "yarn upgrade --latest --exact webpack webpack-cli webpack-dev-server"
  },
  "author": "Eric D Hiller",
  "license": "ISC",
  "dependencies": {
    "@ant-design/compatible": "1.0.2",
    "@ant-design/icons": "4.0.6",
    "@apollo/react-common": "3.1.4",
    "@apollo/react-hoc": "3.1.5",
    "antd": "4.1.4",
    "apollo-cache": "1.3.4",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "apollo-utilities": "1.3.3",
    "bwip-js": "2.0.6",
    "cors": "2.8.5",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.3",
    "graphql-tools": "4.0.7",
    "konva": "5.0.0",
    "nunjucks": "3.2.1",
    "rc-animate": "2.11.1",
    "react": "16.13.1",
    "react-apollo": "3.1.5",
    "react-dom": "16.13.1",
    "react-konva": "16.13.0-0",
    "react-rnd": "10.1.9",
    "react-router-dom": "5.1.2",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.3",
    "@graphql-codegen/introspection": "1.13.3",
